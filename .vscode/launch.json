    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "electron",
            "env": {
                "NODE_ENV": "production"
            },
            "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron",
            "program": "${workspaceFolder}/dist/main.js",
            "cwd": "${workspaceFolder}",
            "sourceMaps": true,
            "protocol": "inspector"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "ts",
            "runtimeArgs": [
                "-r",
                "ts-node/register"
            ],
            "args": [
                "${workspaceFolder}/src/main_process/controller/note.ts"
            ]
        }
    ]
}